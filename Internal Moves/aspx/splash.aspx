#sideNavBox {
			display:none;
		}
		#s4-titlerow {
			border-bottom:1px solid #bfbfbf;
			box-sizing:content-box!important;
		}
		#s4-bodyContainer {
			padding-bottom:0px!important;
		}
		#contentBox {
			margin:0px;
		}
		
		#contentRow {
			padding-top:0px;
		}
		#MSOZoneCell_WebPartWPQ2 {
			margin:0px;
		}
		
		h1#pageTitle {
		    margin-top: 5px;
		    line-height: 20px;
		}
		h1#pageTitle span {
			font-size: 20px;
			font-weight:bold;
		}
		
		a.reportButton:first-child { 
			border-top:1px solid #666;
		}
		
		a.ico-complete {
			background-image:url('https://mcscviper.usmc.mil/sites/vt/CICO/SiteAssets/img/ico-sprite.png');
			background-repeat:no-repeat;
			background-position: 5px 2px;
		}
		
		a.ico-ongoing{
			background-image:url('https://mcscviper.usmc.mil/sites/vt/CICO/SiteAssets/img/ico-sprite.png');
			background-repeat:no-repeat;
			background-position: 5px -25px;
		}
		
		a.reportButton {
			width: 250px;
			display: block;
		    padding: 5px 10px 5px 35px;
		    border-bottom:1px solid #666;
			border-left:1px solid #666;
			border-right:1px solid #666;
			border-top:1px solid #fff;
		    color: #666;
		    background-color: #efefef;
		    font-weight: bold;
		    text-decoration: none;
		}
		
		div#buttons a {
		    display: block;
		    float: left;
		    margin: 10px;
		    padding: 5px 10px;
		    border-radius: 4px;
		    color: white;
		    background-color: maroon;
		    font-weight: bold;
		    text-decoration: none;
		}
		
		#wrapper div[id$='wrap'] {
			float:left;
		}
		
		#left-wrap {
			width:300px;
			border-right:1px solid #bfbfbf;
			min-height:500px;
			height:auto;
			background-color:#d9d9d9;
			background-image:url("https://mcscviper.usmc.mil/sites/vt/CICO/SiteAssets/img/mcsc-logo-trans.png");
			background-position: center bottom;
			background-repeat:no-repeat;
		}

		#left-wrap h3 {
		    text-align: center;
		    color: maroon;
		    font-size: 14px;
		    font-weight: bold;
		}
		
		#left-wrap ul {
		    margin: 0;
		    list-style: none;
		    padding: 0;
		}
		
		#left-wrap ul li {
		    border-top: 1px solid white;
		    border-bottom: 1px solid #a6a6a6;
		    padding: 5px 10px;
		    text-align: center;
		}
		
		#left-wrap a {
		    color: #7f7f7f;
		}
		
		#left-wrap a:hover {
		    text-decoration: none;
		}
		
		#left-wrap ul li:hover {
		    background-color: #ededed;
		}
		
		#left-wrap ul li:first-child {
		    border-top: 0px solid transparent;
		    padding:0;
		}
		
		#left-wrap ul li:last-child {
		    border-bottom: 0px solid transparent;
		    padding:0;
		}

		
		#right-wrap {
			background-color:#f2f2f2;
			padding:20px;
			overflow-y:scroll;
		}
		
		#ctl00_PlaceHolderMain_WikiField div.ms-wikicontent {
			padding-right: 0px!important;
		}
		.ms-rte-layoutszone-inner {
			margin:0px;
		}
		
		p.long-line {
		    font-size: 18px;
		    font-weight: bold;
		    color: maroon;
		    padding-left: 7px;
		    border-bottom: 1px solid maroon;
		    width: 240px;
		    line-height: 20px !important;
		    margin-bottom:0px!important;
		}
		
		p.subtitle {
		    color: maroon;
		    font-style: italic;
		    padding-left: 7px;
		}
		table.ms-listviewtable {
			border:0px solid transparent;
		    border-collapse: separate;
		    width: 100%;
		}
		
#js-listviewthead-WPQ2 tr th {
    font-weight: bold;
    color: black;
}

table[summary='Check Out'] tr td {
    background-color: white;
}

table[summary='Check Out'] tbody tr:first-child td {
    border-top: 1px solid black;
}

table[summary='Check Out'] tbody tr:last-child td {
    border-bottom: 1px solid black;
}

.ms-vh2 a {
    color: #333;
    font-size: 14px;
    font-weight:bold;
}

.ms-alternating {
    background-color: white;
}


